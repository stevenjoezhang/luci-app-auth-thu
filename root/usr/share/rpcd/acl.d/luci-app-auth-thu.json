{
	"luci-app-auth-thu": {
		"description": "Grant access to Auth THU configuration",
		"read": {
			"ubus": {
				"service": ["list"],
				"uci": ["get", "load"]
			},
			"file": {
				"/usr/bin/goauthing": ["exec"],
				"/usr/bin/wget": ["exec"],
				"/bin/uname": ["exec"],
				"/bin/mv": ["exec"],
				"/bin/chmod": ["exec"],
				"/etc/init.d/goauthing": ["exec"]
			}
		},
		"write": {
			"ubus": {
				"uci": ["set", "save", "commit"]
			},
			"uci": [ "goauthing" ]
		}
	}
}
